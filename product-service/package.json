{
  "name": "product",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "jest --watchAll --detectOpenHandles",
    "test:coverage": "jest --maxWorkers=50% --coverage",
    "start:api": "serverless offline -s dev",
    "deploy:dev": "sls deploy -s dev",
    "lint": "eslint --ext .ts --fix .",
    "prettier": "prettier --write --ignore-unknown .",
    "prepare": "cd .. && husky product-service/.husky",
    "clear_jest": "jest --clearCache"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^3.6.2",
    "@middy/http-error-handler": "^5.2.4",
    "@middy/http-json-body-parser": "^3.6.2"
  },
  "devDependencies": {
    "@serverless/event-mocks": "^1.1.1",
    "@serverless/typescript": "^3.38.0",
    "@types/aws-lambda": "^8.10.133",
    "@types/jest": "^29.5.12",
    "@types/node": "^14.18.63",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "esbuild": "^0.14.54",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "json-schema-to-ts": "^3.0.0",
    "prettier": "^3.2.5",
    "serverless": "^3.38.0",
    "serverless-esbuild": "^1.51.0",
    "serverless-offline": "^13.3.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^5.3.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
